    "prisma:studio": "prisma studio",
    "ingest:cars": "tsx scripts/ingest-cars.ts \"data/Cars_Nov_4_2025.csv\"",
    "search": "tsx scripts/search.ts prefs.example.json 10"
  },
  "dependencies": {
    "next": "^14.2.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@prisma/client": "^5.19.1",
    "dotenv": "^16.4.5",
    "zod": "^3.23.8",
    "csv-parse": "^5.5.6",
    "openai": "^4.57.0",
    "@upstash/redis": "^1.28.3",
    "@tanstack/react-query": "^5.59.0",
    "react-hook-form": "^7.53.0",
    "@hookform/resolvers": "^3.9.0",
    "lucide-react": "^0.446.0",
    "recharts": "^2.12.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.43",
    "@playwright/test": "^1.48.2",
    "vitest": "^2.1.4",
    "prisma": "^5.19.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "tailwindcss": "^3.4.14",
    "postcss": "^8.4.47",
    "autoprefixer": "^10.4.20"
  }
